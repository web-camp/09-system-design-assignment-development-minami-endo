<h1>Admins::Orders#index</h1>
<p>Find me in app/views/admins/orders/index.html.erb</p>

<h1>注文履歴一覧</h1>

<table>
	<thead>
		<tr>
			<th>購入日時</th>
			<th>購入者</th>
			<th>注文個数</th>
			<th>注文ステータス</th>
		</tr>
	</thead>
	<tbody>
		<%= orders.each do |order| %>
		<tr>
			<td><%= link_to order.created_at, admins_order_path(order.id) %></td>
			<td><%= order.name %></td>
			<td><%= order.order_products.count %></td>
			<td>注文ステータス</td>
		</tr>
		<% end %>
	</tbody>
</table>

