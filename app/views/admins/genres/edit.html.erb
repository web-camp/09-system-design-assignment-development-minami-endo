<h1>Admins::Genres#edit</h1>
<p>Find me in app/views/admins/genres/edit.html.erb</p>

<h3>ジャンル編集</h3>

<%= form_for(@genre, url: admins_genre_path, method: :patch) do |f| %>
  <span>ジャンル</span>
  <%= f.text_field :name %>
  <%= f.radio_button :is_active, :effectiveness, checked: "checked" %>
    <%= f.label :有効 %>
  <%= f.radio_button :is_active, :unavailable %>
    <%= f.label :無効 %>
  <%= f.submit "変更を保存する", class:"btn btn-primary" %>
<% end %>