<h1>Admins::Genres#index</h1>
<p>Find me in app/views/admins/genres/index.html.erb</p>

<h1>ジャンル一覧・追加</h1>

<%= form_for(@genre, url: admins_genres_path, method: :post) do |f| %>
  <span>ジャンル</span>
  <%= f.text_field :name %>
  <%= f.submit "追加", class:"btn btn-primary" %>
<% end %>


<table>
	<tr>
		<th>ジャンル</th>
		<th>状態</th>
	</tr>
  <%= @genres.each do |genre| %>
	<tr>
		<td><%= genre.name %></td>
		<td>有効</td>
	</tr>
  <% end %>
</table>