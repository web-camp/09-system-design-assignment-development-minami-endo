<h1>Admins::Products#new</h1>
<p>Find me in app/views/admins/products/new.html.erb</p>

<h1>新商品登録</h1>

<p>画像</p>

<%= form_for(@product, url: admins_products_path, method: :post) do |f| %>
  <p><span>商品名</span>
    <%= f.text_field :name %>
</p>
  <span>商品説明</span>
    <%= f.text_area :caption %>
  <p>ジャンル</p>
  <span>税抜価格</span>
    <%= f.text_field :non_taxed_price %>
  <p>販売ステータス</p>

  <%= f.submit "新規登録", class:"btn btn-primary" %>

<% end %>