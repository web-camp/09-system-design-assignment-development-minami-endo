<h1>Admins::Products#index</h1>
<p>Find me in app/views/admins/products/index.html.erb</p>

<h1>商品一覧</h1>
<table>
	<thead>
		<tr>
			<th>商品id</th>
			<th>商品名</th>
		    <th>税抜価格</th>
		    <th>ジャンル</th>
		    <th>ステータス</th>
		</tr>
	</thead>
	<tbody>
		<% @products.each do |product| %>
		<tr>
			<td><%= product.id %></td>
			<td><%= link_to product.name, admins_product_path(product.id) %></td>
			<td><%= product.non_taxed_price %></td>
			<td><%= product.genre.name %></td>
			<td>ステータス</td>
		</tr>
		<% end %>
	</tbody>
</table>

